                    Imatog1.Position = UDim2.new(0.87, 0, 0.65, 0)
                    Imatog1.BorderSizePixel = 0
                    Imatog1.ImageColor3 = UiSettings.SchemeColor
                    Imatog1.Image = "http://www.roblox.com/asset/?id=6035047377"
                    Imatog1.Parent = SliderFrame2
                    
                    local SliderValueFrame = Instance.new("Frame")
                    SliderValueFrame.Name = "SliderValueFrame"
                    SliderValueFrame.AnchorPoint = Vector2.new(0.5, 0.5)
                    SliderValueFrame.Size = UDim2.new(0, 308, 0, 4)
                    SliderValueFrame.Position = UDim2.new(0.4857185, 0, 0.7700002, 0)
                    SliderValueFrame.BorderSizePixel = 0
                    SliderValueFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    SliderValueFrame.Parent = SliderFrame2
                    				local dragging = false

                    local SliderValueFrame1 = Instance.new("Frame")
                    SliderValueFrame1.Name = "SliderValueFrame"
                    SliderValueFrame1.Size = UDim2.new(0.5, 0, 0, 4)
                    SliderValueFrame1.BorderSizePixel = 0
                    SliderValueFrame1.BackgroundColor3 = UiSettings.SchemeColor
                    SliderValueFrame1.Parent = SliderValueFrame
                    
                    local SliderValueFrame2 = Instance.new("ImageButton")
                    SliderValueFrame2.Name = "SliderValueFrame"
                    SliderValueFrame2.Size = UDim2.new(0, 15, 0, 15)
			 	  SliderValueFrame2.BackgroundTransparency= 1
                    SliderValueFrame2.Position = UDim2.new(0.5, 0, 0, 3)
                    SliderValueFrame2.BorderSizePixel = 0
                    SliderValueFrame2.BackgroundColor3 = UiSettings.SchemeColor
                    SliderValueFrame2.Parent = SliderValueFrame
                    SliderValueFrame2.Image =
        "https://www.roblox.com/asset-thumbnail/image?assetId=" ..
        UiSettings.icon .. "&width=0&height=0&format=png"

                    local MHEENAME12 = Instance.new("UICorner")
                    MHEENAME12.Name = "MHEENAME"
                    MHEENAME12.CornerRadius = UDim.new(0, 4)
                    MHEENAME12.Parent = SliderFrame

                    local Value = default
                    local mouse = game:GetService("Players").LocalPlayer:GetMouse()
                    local function move(input)
					local pos =
						UDim2.new(
							math.clamp((input.Position.X - SliderFrame.AbsolutePosition.X) / SliderFrame.AbsoluteSize.X, 0, 1),
							-6,
							0,
							-6
						)
					local pos1 =
						UDim2.new(
							math.clamp((input.Position.X - SliderFrame.AbsolutePosition.X) / SliderFrame.AbsoluteSize.X, 0, 1),
							0,
							0,
							4
						)
					SliderValueFrame1.Size = pos1
					SliderValueFrame2.Position = pos
					
                        local value = math.floor(((pos.X.Scale * max) / max) * (max - min) + min)
                        TextBox.Text = tostring(value)
                        Value = value
                        pcall(callback, value)
                        if save then
                            Config[flag] = value
                            saveConfig()
                        end
                    end
                    SliderValueFrame2.InputBegan:Connect(
                        function(input)
                            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                                dragging = true
                            end
                        end
                    )
                    SliderValueFrame2.InputEnded:Connect(
                        function(input)
                            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                                dragging = false
                            end
                        end
                    )
                    game:GetService("UserInputService").InputChanged:Connect(
                        function(input)
                            if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                                move(input)
                            end
                        end
                    )

                    TextBox.FocusLost:Connect(
                        function(ep)
                            if ep then
                                if #TextBox.Text > 0 then
                                    Value = math.clamp(TextBox.Text, min, max)

                                    local pos = UDim2.new(math.clamp((Value) / max, 0, 1), -3, 0,-6)
                                    SliderValueFrame2.Position = pos
                                    SliderValueFrame1.Size = UDim2.new((Value or 0) / max, 0, 0, 4)
                                    pcall(callback, Value)
                                    if save then
                                        Config[flag] = Value
                                        saveConfig()
                                    end
                                end
                            end
                        end
                    )
                    local A = Instance.new("IntValue")
                    A.Parent = SliderFrame
                    A.Value = default
                    TextBox:GetPropertyChangedSignal("Text"):Connect(
                        function()
                            A.Value = TextBox.Text
                        end
                    )
                    TextBox.FocusLost:Connect(
                        function(ep)
                            if ep then
                                if A.Value > max then
                                    A.Value = max
                                    Value = max
                                    TextBox.Text = max
                                end
                                if A.Value < min then
                                    A.Value = min
                                    Value = min
                                    TextBox.Text = min
                                end
                            end
                        end
                    )

                    Imatog1.MouseButton1Click:Connect(
                        function()
                            Value = math.clamp(Value + 1, 1, max)

                            SliderValueFrame2.Position = UDim2.new(math.clamp((Value) / max, 0, 1), 0, 0, -6)
                            SliderValueFrame1.Size = UDim2.new((Value or 0) / max, 3, 0, 4)
                            TextBox.Text = Value
                            pcall(
                                function()
                                    callback(Value)
                                    if save then
                                        Config[flag] = Value
                                        saveConfig()
                                    end
                                end
                            )
                        end
                    )

                    Imatog.MouseButton1Click:Connect(
                        function()
                            Value = math.clamp(Value - 1, min, max)
                            local pos = UDim2.new(math.clamp((Value) / max, 0, 1), 0, 0, -6)
                            SliderValueFrame2.Position = pos
                            SliderValueFrame1.Size = UDim2.new((Value or 0) / max, 3, 0, 4)
                            TextBox.Text = Value
                            pcall(
                                function()
                                    callback(Value)
                                    if save then
                                        Config[flag] = Value
                                        saveConfig()
                                    end
                                end
                            )
                        end
                    )

                    if default then
                        if not default then value = math.clamp(min/max) end
                        local pos = UDim2.new(math.clamp((default) / max, 0, 1), 0, 0, -6)
                        SliderValueFrame2.Position = pos
                        SliderValueFrame1.Size = UDim2.new((default or 0) / max, 3, 0, 4)

                        TextBox.Text = default
                        pcall(
                            function()
                                callback(default)
                            end
                        )
                    end
                    function SliderFunc:Update(value)
                        local pos = UDim2.new(math.clamp((value) / max, 0, 1), 0, 0, 0)
                        SliderValueFrame2.Position = pos
                        SliderValueFrame1.Size = UDim2.new((value or 0) / max, 0, 0, 4)
                        if save then
                            Config[flag] = value
                            saveConfig()
                        end
                        TextBox.Text = value
                        pcall(
                            function()
                                callback(value)
                            end
                        )
                    end
                    return SliderFunc
                end
