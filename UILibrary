                    end
                    function DropFunc:Clear()
                        TextLabel6.Text = Title
                        for i, v in next, Scollist:GetChildren() do
                            if v.Name == "Frame" then
                                v:Destroy()
                            end
                        end
                        if DropTog == true then
                            TextLabel6.Text = Title
                        end
                    end
                    return DropFunc
                end
