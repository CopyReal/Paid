        function pagecontent:Image3D(title,Parenta)
            local Grid1 = Instance.new("Frame")
            Grid1.Name = "Grid1"
            Grid1.Size = UDim2.new(0, 459, 0, 328)
            Grid1.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
            Grid1.Parent = FFServer1
            Grid1.BorderSizePixel = 0
            Grid1.BackgroundTransparency = 1
            Grid1.Visible = true

            local TextLabel = Instance.new("TextLabel")
            TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
            TextLabel.Size = UDim2.new(0, 423, 0, 20)
            TextLabel.BackgroundTransparency = 1
            TextLabel.Position = UDim2.new(0, 50, 0, 115)
            TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.FontSize = Enum.FontSize.Size18
            TextLabel.TextSize = 17
            TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.Text = title
            TextLabel.Font = Enum.Font.GothamSemibold
            TextLabel.Parent = Grid1
            local UITextSizeConstraint = Instance.new("UITextSizeConstraint")
            UITextSizeConstraint.MaxTextSize = 16
            UITextSizeConstraint.MinTextSize = 5
            UITextSizeConstraint.Parent = TextLabel
        
                if Parenta:FindFirstChild("Humanoid") then
                    local ViewportFrame = Instance.new("ViewportFrame")
                    ViewportFrame.Parent = Grid1
                    ViewportFrame.BackgroundTransparency = 1
                    ViewportFrame.Size = UDim2.new(0,100,0,100)
                    local Preview = Parenta:Clone()
                    Preview.Parent = ViewportFrame
            
                    local Camera = Instance.new("Camera")
                    Camera.Parent = ViewportFrame
                    Camera.CFrame = Preview.Head.CFrame + Preview.Head.CFrame.LookVector
                    Camera.CFrame = CFrame.new(Camera.CFrame.Position,Preview.Head.Position)
                    ViewportFrame.CurrentCamera = Camera
                end
        end

        function pagecontent:Toggle3d(title,Paretn,save,flag,default,lock,callback)
            local Grid1 = Instance.new("Frame")
            Grid1.Name = "Grid1"
            Grid1.Size = UDim2.new(0, 459, 0, 328)
            Grid1.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
            Grid1.Parent = FFServer1
            Grid1.BorderSizePixel = 0
            Grid1.BackgroundTransparency = 1
            Grid1.Visible = true
            local TextLabel = Instance.new("TextLabel")
            TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
            TextLabel.Size = UDim2.new(0, 423, 0, 20)
            TextLabel.BackgroundTransparency = 1
            TextLabel.Position = UDim2.new(0, 50, 0, 115)
            TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.FontSize = Enum.FontSize.Size18
            TextLabel.TextSize = 17
            TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.Text = title
            TextLabel.Font = Enum.Font.GothamSemibold
            TextLabel.Parent = Grid1
            local UITextSizeConstraint = Instance.new("UITextSizeConstraint")
            UITextSizeConstraint.MaxTextSize = 16
            UITextSizeConstraint.MinTextSize = 5
            UITextSizeConstraint.Parent = TextLabel
