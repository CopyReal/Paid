            ImageLabel1.AnchorPoint = Vector2.new(0.5, 0.5)
            ImageLabel1.BackgroundTransparency = 1
            ImageLabel1.Position = UDim2.new(0.5, 0, 0.5, 0)
            ImageLabel1.BorderSizePixel = 0
            ImageLabel1.Size = UDim2.new(0,100,0,100)
            ImageLabel1.Image = ""
            ImageLabel1.Parent = Grid1


            local GridFunc = {}
            if Paretn:FindFirstChild("Humanoid") then
                local ViewportFrame = Instance.new("ViewportFrame")
                ViewportFrame.Parent = Grid1
                ViewportFrame.BackgroundTransparency = 1
                ViewportFrame.Size = UDim2.new(0,100,0,100)
                local Preview = Paretn:Clone()
                Preview.Parent = ViewportFrame
                local asdasasdasdas = Instance.new("UICorner")
                asdasasdasdas.CornerRadius = UDim.new(0, 10)
                asdasasdasdas.Parent = ViewportFrame
                local Camera = Instance.new("Camera")
                Camera.Parent = ViewportFrame
                Camera.CFrame = Preview.Head.CFrame + Preview.Head.CFrame.LookVector
                Camera.CFrame = CFrame.new(Camera.CFrame.Position,Preview.Head.Position)
                ViewportFrame.CurrentCamera = Camera
            end
            local lockerframe = Instance.new("Frame")
            lockerframe.Name = "lockerframe"
            lockerframe.AnchorPoint = Vector2.new(0.5, 0.5)
            lockerframe.Size = UDim2.new(0, 100, 0, 100)
            lockerframe.BackgroundTransparency = 0.5
            lockerframe.Position = UDim2.new(0.5, 0, 0.5, 0)
            lockerframe.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            lockerframe.Parent = Grid1
            lockerframe.Visible = false

            local UICorner3 = Instance.new("UICorner")
            UICorner3.CornerRadius = UDim.new(0, 10)
            UICorner3.Parent = Grid1

            local asdas = Instance.new("UICorner")
            asdas.CornerRadius = UDim.new(0, 10)
            asdas.Parent = lockerframe

            local UIStroke2 = Instance.new("UIStroke")
            UIStroke2.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            UIStroke2.Color = Color3.fromRGB(255, 255, 255)
            UIStroke2.Parent = Grid1
            UIStroke2.Thickness = 3

            local ImageLabelasd1 = Instance.new("ImageLabel")
            ImageLabelasd1.AnchorPoint = Vector2.new(0.5, 0.5)
            ImageLabelasd1.BackgroundTransparency = 1
            ImageLabelasd1.Position = UDim2.new(0.5, 0, 0.5, 0)
            ImageLabelasd1.Size = UDim2.new(0,50,0,50)
            ImageLabelasd1.BorderSizePixel = 0
            ImageLabelasd1.Image = "rbxassetid://8140932860"
            ImageLabelasd1.Parent = lockerframe

            local Lock = lock

            function GridFunc:Lock()lockerframe.Visible = true Lock = true end
            function GridFunc:Unlock()lockerframe.Visible = false Lock = false end
            local Toggled = default
            function GridFunc:Update(state)
                callback(state)
                if save then
                    Config[flag] = state
                    saveConfig()
                end
                    TweenService:Create(
                        UIStroke2,
                        TweenInfo.new(.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                        {Color = state and UiSettings.SchemeColor or Color3.fromRGB(255,255,255)}
                    ):Play()
            end

            if lock then
                GridFunc:Lock()
            end
            if Lock == false then
                ImageLabel1.MouseButton1Click:Connect(function()
                Toggled = not Toggled
                GridFunc:Update(Toggled)
            end)
            if default then
                Toggled = default
                GridFunc:Update(Toggled)
            end
        end
            return GridFunc
        end
