                    TitilFrame.TextColor3 = Color3.fromRGB(255, 255, 255)
                    TitilFrame.Text = Title
                    TitilFrame.Font = Enum.Font.GothamSemibold
                    TitilFrame.TextXAlignment = Enum.TextXAlignment.Left
                    TitilFrame.Parent = SliderFrame2
                    
                    local MHEENAME11 = Instance.new("UICorner")
