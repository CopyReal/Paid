                    local UICorner6 = Instance.new("UICorner")
                    UICorner6.Name = ""
                    UICorner6.CornerRadius = UDim.new(0, 4)
                    UICorner6.Parent = TextBox
                    TextBox.FocusLost:Connect(
                        function(ep)
                            if ep then
                                if #TextBox.Text > 0 then
                                    pcall(callback, TextBox.Text)
                                    if save then
                                        Config[flag] = TextBox.Text
                                        saveConfig()
                                    end
                                    if disapper then
                                        TextBox.Text = ""
                                        else 
                                        TextBox.Text = TextBox.Text	
                                    end
                                end
                            end
                        end
                    )
			function TexTFUCNC:Update(value)
				if disapper then
					TextBox.Text = ""
                    else 
                    TextBox.Text = Value				
                end
				callback(value)
			end
		   return TexTFUCNC
                end
                function items:Slider(Title, min, max,save,flag, default, callback)
                    if default == nil or default == "" then
                        default = (min / max)
                    end
                    local SliderFunc = {}
