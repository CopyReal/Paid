                        TextButton4.MouseButton1Click:Connect(
                            function()
                                if not table.find(DropTable, v) then
                                    table.insert(DropTable, v)
                                    callback(DropTable, v)
                                    if save then
                                        Config[flag] = DropTable
                                        saveConfig()
                                    end
                                    TextLabel6.Text = table.concat(DropTable, ", ")
                                else
                                    for i2, v2 in pairs(DropTable) do
                                        if v2 == v then
                                            table.remove(DropTable, i2)
                                            TextLabel6.Text = table.concat(DropTable, ", ")
                                        end
                                    end
                                    callback(DropTable, v)
                                    if save then
                                        Config[flag] = unpack(DropTable)
                                        saveConfig()
                                    end
                                end
            
                            end
                        )
                        Scollist.CanvasSize = UDim2.new(0, 0, 0, UIListLayout3.AbsoluteContentSize.Y)
                    end
                    function DropFunc:Add(text)
                        local Frame = Instance.new("Frame")
                        Frame.Size = UDim2.new(0, 423, 0, 35)
                        Frame.BackgroundTransparency = 1
                        Frame.Position = UDim2.new(0.033557, 0, 0.32, 0)
                        Frame.BorderSizePixel = 0
                        Frame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
                        Frame.Parent = Scollist

                        local TextButton4 = Instance.new("TextButton")
                        TextButton4.Size = UDim2.new(0, 423, 0, 35)
                        TextButton4.BackgroundTransparency = 1
                        TextButton4.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
                        TextButton4.AutoButtonColor = false
                        TextButton4.FontSize = Enum.FontSize.Size14
                        TextButton4.TextSize = 14
                        TextButton4.TextColor3 = Color3.fromRGB(0, 0, 0)
                        TextButton4.Text = " "
                        TextButton4.Font = Enum.Font.SourceSans
                        TextButton4.Parent = Frame

                        local TextLabel7 = Instance.new("TextLabel")
                        TextLabel7.Size = UDim2.new(0, 129, 0, 36)
                        TextLabel7.BackgroundTransparency = 1
                        TextLabel7.Position = UDim2.new(0.1075269, 0, 0, 0)
                        TextLabel7.BorderSizePixel = 0
                        TextLabel7.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        TextLabel7.FontSize = Enum.FontSize.Size18
                        TextLabel7.TextSize = 17
                        TextLabel7.TextColor3 = Color3.fromRGB(102, 102, 102)
                        TextLabel7.Text = text
                        TextLabel7.Font = Enum.Font.GothamSemibold
                        TextLabel7.Parent = TextButton4

                        local ImageLabel5 = Instance.new("ImageLabel")
                        ImageLabel5.Size = UDim2.new(0, 22, 0, 22)
                        ImageLabel5.BackgroundTransparency = 1
                        ImageLabel5.Position = UDim2.new(0.8014385, 0, 0.109589, 0)
                        ImageLabel5.BorderSizePixel = 0
                        ImageLabel5.ImageColor3 = Color3.fromRGB(102, 102, 102)
                        ImageLabel5.Image = "http://www.roblox.com/asset/?id=6034287594"
                        ImageLabel5.Parent = TextButton4

                        local TextLabel8 = Instance.new("TextLabel")
                        TextLabel8.Size = UDim2.new(0, 190, 0, 3)
                        TextLabel8.BackgroundTransparency = 0.5
                        TextLabel8.Position = UDim2.new(0.1, 0, 0.9, 0)
                        TextLabel8.BorderSizePixel = 0
                        TextLabel8.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
                        TextLabel8.FontSize = Enum.FontSize.Size14
                        TextLabel8.TextSize = 14
                        TextLabel8.TextColor3 = Color3.fromRGB(0, 0, 0)
                        TextLabel8.Text = " "
                        TextLabel8.Font = Enum.Font.SourceSans
                        TextLabel8.Parent = TextButton4

                        local UICorner2 = Instance.new("UICorner")
                        UICorner2.Name = ""
                        UICorner2.CornerRadius = UDim.new(0, 300)
                        UICorner2.Parent = TextLabel8
                        TextButton4.MouseButton1Click:Connect(
                            function()
                                if not table.find(DropTable, v) then
                                    table.insert(DropTable, v)
                                    callback(DropTable, v)
                                    if save then
                                        Config[flag] = DropTable
                                        saveConfig()
                                    end
                                    TextLabel6.Text = table.concat(DropTable, ", ")
                                else
                                    for i2, v2 in pairs(DropTable) do
                                        if v2 == v then
                                            table.remove(DropTable, i2)
                                            TextLabel6.Text = table.concat(DropTable, ", ")
                                        end
                                    end
                                    callback(DropTable, v)
                                    if save then
                                        Config[flag] = DropTable
                                        saveConfig()
                                    end
                                end
            
                            end
                        )
                        Scollist.CanvasSize = UDim2.new(0, 0, 0, UIListLayout3.AbsoluteContentSize.Y)
                    end
                    function DropFunc:Clear()
                        TextLabel6.Text = Title
                        for i, v in next, Scollist:GetChildren() do
                            if v.Name == "Frame" then
                                v:Destroy()
                            end
                        end
                        if DropTog == true then
                            TextLabel6.Text = Title
                        end
                    end
                    return DropFunc
            end

            return items
        end
        return pagecontent
    end
    return tabs
end
return library
