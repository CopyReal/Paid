                    MHEENAME11.Parent = SliderFrame2
                    
                    local CustomValueMain = Instance.new("Frame")
                    CustomValueMain.Name = "CustomValueMain"
                    CustomValueMain.AnchorPoint = Vector2.new(0.5, 0.5)
