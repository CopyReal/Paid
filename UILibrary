                    local Canclick = true
                    function TogFunc:Lock()
                        Canclick = false
                        lockerframe.BackgroundTransparency = 0.5
                        ImageLabel1.ImageTransparency = 0
                    end
                    function TogFunc:Unlock()
                        Canclick = true
                        lockerframe.BackgroundTransparency = 1
                        ImageLabel1.ImageTransparency = 1
                    end

                    if lock then
					TogFunc:Lock()
                    end

                    function TogFunc:Update(bool)
                        TweenService:Create(
                            Togmain,
                            TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {
                                BackgroundColor3 = bool and UiSettings.SchemeColor or
                                Color3.fromRGB(247, 64, 64)
                            }
                        ):Play()

                            TweenService:Create(
                                Togmain,
                                TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                                {Position = bool and UDim2.new(0, 24, 0, 1) or UDim2.new(0.05, 0, 0, 1)}
                            ):Play()

                            pcall(callback, bool)
                            if save then
                                Config[flag] = bool
                                saveConfig()
                            end
                    end
                    if default then
                        Toggled = default
                        TogFunc:Update(Toggled)
                    else
                        Toggled = default
                        TogFunc:Update(Toggled)
                    end
                    local Toggled = false
                    if Canclick then
                        MainToggle.MouseButton1Click:Connect(
                            function()
                                Toggled = not Toggled
                                TogFunc:Update(Toggled)

                            end
                        )
                    end
                    return TogFunc
                end
